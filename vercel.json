{
  "version": 2,
  "builds": [
    {"src": "**/*.html", "use": "@vercel/static"},
    {"src": "Asset/**", "use": "@vercel/static"},
    {"src": "Font/**", "use": "@vercel/static"},
    {"src": "img/**", "use": "@vercel/static"},
    {"src": "Index/**", "use": "@vercel/static"}
  ],
  "routes": [
    {"src": "^/$", "dest": "/Index.html"},
    {"src": "/(.*)", "dest": "/$1"}
  ]
}
